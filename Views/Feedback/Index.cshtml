@model IEnumerable<CruiseshipApp.Models.Feedback>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Feedback/Feedback_LayoutPage.cshtml";
}

<h2>Feedback</h2>


@if (TempData["AlertMessage"] != null)
{
    <div class="alert-success">
        <strong>Success !</strong> @TempData["AlertMessage"]
    </div>
}
<p>
    @Html.ActionLink("Add New", "Create")
</p>
<table class="table">
    <tr>
@*        <th>
            @Html.DisplayNameFor(model => model.Voyager_id)
        </th>*@
        <th>
            @Html.DisplayNameFor(model => model.Date)
        </th>
        <th>
            Feedback
            @*@Html.DisplayNameFor(model => model.Feedback1)*@
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
@*            <td>
                @Html.DisplayFor(modelItem => item.Voyager_id)
            </td>*@
            <td>
                @Html.DisplayFor(modelItem => item.Date)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Feedback1)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.Feedback_id }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.Feedback_id })
            </td>
        </tr>
    }

</table>
